{
  "name": "hentaits-next",
  "version": "0.3.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "docker:build": "docker build -t asusguy94/hentaits .",
    "docker:build-beta": "docker build -t asusguy94/hentaits:beta .",
    "docker:build-alpha": "docker build -t asusguy94/hentaits:alpha .",
    "docker:push": "docker push asusguy94/hentaits",
    "docker:push-beta": "docker push asusguy94/hentaits:beta",
    "docker:push-alpha": "docker push asusguy94/hentaits:alpha",
    "docker:remove": "docker image prune -f",
    "docker": "yarn docker:build && yarn docker:push && yarn docker:remove",
    "docker-beta": "yarn docker:build-beta && yarn docker:push-beta && yarn docker:remove",
    "docker-alpha": "yarn docker:build-alpha && yarn docker:push-alpha && yarn docker:remove",
    "docker:watch-beta": "nodemon --exec \"yarn docker-beta\"",
    "docker:watch-alpha": "nodemon --exec \"yarn docker-alpha\""
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",

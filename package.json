    "@prisma/client": "^5",
    "@tanstack/react-query": "^5",
    "axios": "^1.3.5",
    "capitalize": "^2.0.4",
    "dayjs": "^1.11.7",
    "fake-useragent": "^1.0.1",
    "ffmpeg-generate-video-preview": "^1.0.3",
    "fluent-ffmpeg": "^2.1.2",
    "get-video-dimensions": "^1.0.0",
    "hls.js": "^1.4.0",
    "next": "^14",
    "node-fetch": "^3.3.1",
    "plyr": "^3.7.8",
    "prisma": "^5",
    "pusher": "^5.1.3",
    "pusher-js": "^8.3.0",
    "rctx-contextmenu": "^1.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-flip-toolkit": "^7.0.17",
    "react-scroll-to-top": "^3.0.0",
    "react-toastify": "^9.1.3",
    "react-tooltip": "^5",
    "react-use": "^17.4.0",
    "react-virtuoso": "^4.2.1",
    "sass": "^1.62.0",
    "sharp": "^0.33.0",
    "usehooks-ts": "^2.9.1",
    "zod": "^3.21.4"
